{
  "name": "everythingos",
  "version": "2.0.0",
  "type": "module",
  "description": "LLM-Agnostic Multi-Agent Operating System - From chatbots to robot swarms",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "api": "tsx src/api/server.ts",
    "demo": "tsx examples/demo-simple.ts",
    "demo:cli": "tsx examples/demo-cli.ts",
    "sim": "tsx examples/demo-robot.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src --ext .ts"
},

  "keywords": ["multi-agent", "llm", "workflow", "automation", "ai-agents", "robotics", "swarm", "iot"],
  "author": "m0rs3",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  },
  "dependencies": {
    "express": "^4.18.2",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
